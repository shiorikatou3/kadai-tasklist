<h1>タスク一覧</h1>
<P>ようこそ、<%= current_user.name %>さん</P>

<ul>
  <% @tasks.each do |task| %>
    <li><%= link_to task.id, task %> : <%= task.content %> &gt; <%= task.status %></li>
  <% end %>
</ul>
<%= paginate @tasks %>

<%= link_to "新規タスクの作成", new_task_path %><br>
<%= link_to "Log out", logout_path, method: :delete %>

